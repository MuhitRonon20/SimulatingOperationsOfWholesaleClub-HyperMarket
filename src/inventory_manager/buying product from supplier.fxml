      <TextField fx:id="quantityTF" layoutX="177.0" layoutY="134.0" />
      <Label layoutX="89.0" layoutY="176.0" text="unitPrice" />
      <TextField fx:id="unitPriceTF" layoutX="177.0" layoutY="172.0" />

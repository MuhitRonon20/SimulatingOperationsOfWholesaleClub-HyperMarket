<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventory_manager.OrderRestockingController">
   <children>
      <Label layoutX="82.0" layoutY="41.0" text="Product" />
      <Label layoutX="82.0" layoutY="75.0" text="supplier" />
      <Label layoutX="82.0" layoutY="113.0" text="quantity" />
      <Label layoutX="82.0" layoutY="153.0" text="delivery" />
      <ComboBox fx:id="productCB" layoutX="150.0" layoutY="29.0" onAction="#productOnMouseClicked" prefWidth="150.0" />
      <ComboBox fx:id="supplierCB" layoutX="150.0" layoutY="71.0" onAction="#supplierOnMouseClicked" prefWidth="150.0" />
      <TextField fx:id="quantityTF" layoutX="151.0" layoutY="109.0" onAction="#quantityOnMouseClicked" />
      <DatePicker fx:id="deliveryDP" layoutX="139.0" layoutY="149.0" />
      <Button fx:id="orderButton" layoutX="113.0" layoutY="213.0" mnemonicParsing="false" onAction="#orderOnMouseClicked" text="order" />
      <Button fx:id="cancelButton" layoutX="261.0" layoutY="213.0" mnemonicParsing="false" onAction="#cancelOnMouseClicked" text="Cancel" />
   </children>
</AnchorPane>
